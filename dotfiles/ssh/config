        HostName stockholm-exit-node
        IdentityFile ~/.ssh/johan-stockholm.pem

Host vpn-tailscale-nat-ohio
        User ec2-user
        HostName ohio-exit-node
        IdentityFile ~/.ssh/johan-ohio.pem


