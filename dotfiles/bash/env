export JAVA_HOME=/Library/Java/Home

export PATH=$HOME/bin:$PATH:/usr/local/sbin:$SCALA_HOME/bin:$MAGLEV_HOME/bin

export EDITOR="/Users/andersjanmyr/bin/v -f"

export CLICOLOR=1

# RVM
if [[ -s ~/.rvm/scripts/rvm ]] ; then
        source ~/.rvm/scripts/rvm ;
fi
[[ -s $rvm_path/scripts/completion ]] && source $rvm_path/scripts/completion

if [ -f /usr/local/etc/bash_completion.d/git-completion.bash ]; then
    . /usr/local/etc/bash_completion.d/git-completion.bash  
elif [ -f /etc/bash_completion.d/git ]; then
    . /etc/bash_completion.d/git
fi

# Don't use this version of rake-completion, too slow
# source /etc/bash_completion.d/rake-completion.sh

export UDS_HOME=~/tmp

export PGDATA=/usr/local/var/postgres

export NODE_PATH=/usr/local/lib/node:/usr/local/lib/node_modules:/usr/local/lib/jsctags
export NODE_ENV=development

export HISTIGNORE="&:[a-z]:[a-z][a-z]:[a-z][a-z][a-z]"

export SHOWOFF_EVAL_RUBY=1

# Rubinius
export RBXOPT='-X19'

export AWS_CREDENTIALS_FILE=/Users/andersjanmyr/.aws-credentials

[[ -s `rvm gemdir`/bin/tmuxinator_completion ]] && source `rvm gemdir`/bin/tmuxinator_completion

